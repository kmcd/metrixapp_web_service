<script type="text/javascript" src="/swfobject.js"></script>

<ol id='nav'>
   <li>
      <%= link_to 'Add event', new_report_path %>
   </li>
   <li>
      <%= link_to 'Download CSV', report_path(:id => params[:id], :format => :csv) %>
   </li>
</ol>

<div id="chart">
   <p>You need to upgrade your Flash Player</p>
</div>

<p class='hint'>
   Hint: hover your mouse over the graph. You can drag the diamonds on the 
   bottom left and right to change the time scale. Also, hover over the plot
   line for for daily totals.
</p>

<script type="text/javascript">
// <![CDATA[		
   // Heroku has a problem loading swf files so amstock.swf renamed to amstock.html
   var so = new SWFObject("/amstock.html", "amstock", "800", "500", "8", "#FFFFFF");
   so.addVariable("settings_file", "/reports/<%= params[:id] %>.xml");
   so.write("chart");
// ]]>
</script>

<%= metrixapp_log "Dashboard view" %>
